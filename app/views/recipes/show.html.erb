
<div class="recipe_list">
  <p id="recipeTitleShow">
    <%= @recipe.title%>
  </p>
  <p>
  <li id="recipe_ingredients">
    <ol>
    <% @boop = @recipe.ingredients.scan(/"[^"]*"|'[^']*'|[^"'\s\W]+/) %>
    <% @boop.each do |woo| %>

        <li>
    <%=  woo.to_s.gsub('"','' )%>
  </li>
    <% end %>
  </ol>
  </li>
  </p>
</div>
<p id="showLinks">
  <%=link_to "Delete List", recipe_path(@recipe.id),:method => :delete, :id => "editList"%>
</p>
